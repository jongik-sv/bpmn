{
  "name": "bpmn-collaborative-editor",
  "version": "0.1.0",
  "description": "Real-time collaborative BPMN editor",
  "main": "src/index.js",
  "scripts": {
    "dev": "webpack-dev-server --static=public --open",
    "build": "webpack --mode production",
    "start": "npm run dev",
    "ws-server": "node websocket-server.js",
    "dev:full": "concurrently \"npm run ws-server\" \"npm run dev\"",
    "collab": "npm run dev:full",
    "setup-db": "node scripts/setup-database.js",
    "db:test": "node -e \"import('./scripts/setup-database.js').then(m => m.testConnection())\""
  },
  "keywords": [
    "bpmn",
    "collaborative",
    "editor",
    "real-time",
    "crdt"
  ],
  "author": "BPMN Collaborative Editor Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^9.1.0",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^7.1.2",
    "dotenv-webpack": "^8.1.1",
    "html-webpack-plugin": "^5.6.0",
    "raw-loader": "^4.0.0",
    "style-loader": "^4.0.0",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0",
    "ws": "^8.18.0",
    "y-websocket": "^2.1.0",
    "yjs": "^13.6.27"
  },
  "dependencies": {
    "@bpmn-io/properties-panel": "^3.24.0",
    "@supabase/supabase-js": "^2.50.3",
    "@vscode/codicons": "^0.0.36",
    "bpmn-js": "^18.6.2",
    "bpmn-js-properties-panel": "^5.38.0",
    "diagram-js": "^15.1.0",
    "jquery": "^3.7.1",
    "lodash.debounce": "^4.0.8",
    "min-dash": "^4.2.1",
    "y-protocols": "^1.0.6"
  }
}
